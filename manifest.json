{

  "manifest_version": 2,
  "name": "RGB YouTube",
  "version": "2.1",

  "description": "Makes the YouTube progress bar cycle through different RGB colors.",

  "browser_specific_settings": {
    "gecko": {
      "id": "tensor@arrayinamatrix.xyz",
      "strict_min_version": "48.0"
    }
  },

  "icons": {
    "48": "icons/icon-48.jpg",
    "96": "icons/icon-96.jpg"
  },

  "content_scripts": [{
    "matches": ["*://*.youtube.com/*"],
    "js": ["index.js"],
    "css": ["style.css"]
  }]

}